#!/usr/bin/perl
$dir=`pwd`;
chomp $dir;
my $x='';
$x = $dir;
if ($x =~ m!/bgp/!)
{
	$x =~ s!/bgp/!/8.0-branch/!;
}
elsif ($x =~ m!/8.0-branch/!)
{
	$x =~ s!/8.0-branch/!/bgp/!;
}
else 
{
	die "Not in a version'd path";
}

print "diff -w @ARGV[0] $x/@ARGV[0]\n";
print `diff -w @ARGV[0] $x/@ARGV[0]`;

